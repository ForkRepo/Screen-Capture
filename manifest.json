{
    "manifest_version": 2,
    "default_locale": "zh_CN",
    "description": "__MSG_extDesp__",
    "icons": {
      "256": "images/logo.png",
      "128": "images/logo_128.png",
      "64": "images/logo_128.png",
      "32": "images/logo_128.png",
      "16": "images/logo_128.png"
    },
   "manifest_version": 2,
   "name": "__MSG_extName__",
    "version": "1.0.2",
    "background": {
        "scripts": [ "js/background.js" ]
    },
    "commands": {
        "start-capture": {
            "suggested_key": {
                "default": "Ctrl+Shift+1"
            },
            "description": "__MSG_start__",
            "global": true
        }
    },
    "permissions": [ "tabs", "clipboardWrite", "unlimitedStorage", "desktopCapture", "background" ],
    "browser_action": {
        "default_icon": "images/logo.png",
        "default_popup": "layout/popup.html"
    }
}